{
  "name": "@cypress/instrument-cra",
  "version": "1.0.0",
  "description": "Little module for CRA applications to instrument code without ejecting react-scripts",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "start": "react-scripts -r . start",
    "cy:open": "cypress open",
    "dev": "start-test 3000 cy:open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/instrument-cra.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "cra",
    "react-scripts",
    "istanbul"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cypress-io/instrument-cra/issues"
  },
  "homepage": "https://github.com/cypress-io/instrument-cra#readme",
  "dependencies": {
    "babel-plugin-istanbul": "5.2.0",
    "debug": "4.1.1"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "@cypress/code-coverage": "1.8.0",
    "cypress": "3.4.1",
    "istanbul-lib-coverage": "2.0.5",
    "nyc": "14.1.1",
    "start-server-and-test": "1.9.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "exclude": "src/serviceWorker.js"
  }
}
